<h1>ラベル一覧</h1>
<p><%= notice %></p>
<table>
  <tr>
    <th>カテゴリ</th>
  </tr>

<% @labels.each do |label| %>
  <tr>
    <td><%= label.category %></td>  
    <td><%= link_to '詳細',label_path(label.id) %></td>
    <td><%= link_to "編集",edit_label_path(label.id) %></td>
    <td><%= link_to "削除",label_path(label.id),method: :delete %></td>
  </tr>
<% end %>
</table>
<br>
<%= link_to '新しいカテゴリを登録する', new_label_path %>